    <record id="academy_training_resource_word" model="academy.training.resource">
        <field name="name">Microsoft Word</field>
        <field name="description"><![CDATA[Microsoft Word Icon]]></field>
        <field name="active" eval="True"/>
        <field name="manager_id" ref="base.user_root"/>
        <field name="last_update" eval="time.strftime('%Y-%m-%d')"/>
        <field name="ir_attachment_ids" eval="[(4, ref('ir_attachment_academy_resource_word'))]"/>
        <field name="kind_id" ref="academy_base.academy_training_resource_kind_1"/>
    </record>

