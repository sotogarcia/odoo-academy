<?xml version="1.0" encoding="UTF-8"?>

<odoo noupdate="0">

        <record id="academy_timesheets_training_session_draft" model="mail.message.subtype">
            <field name="name">Schedule session temporarily removed</field>
            <field name="res_model">academy.training.session</field>
            <field name="relation_field">state</field>
            <field name="default" eval="True"/>
            <field name="description">The session has been temporarily removed from the schedule</field>
        </record>

        <record id="academy_timesheets_training_session_changed" model="mail.message.subtype">
            <field name="name">Schedule session has been changed</field>
            <field name="res_model">academy.training.session</field>
            <field name="relation_field" />
            <field name="default" eval="True"/>
            <field name="description">The schedule session has been changed</field>
        </record>

</odoo>
