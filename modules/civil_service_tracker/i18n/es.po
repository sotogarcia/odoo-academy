"            <a style=\"text-decoration: none; color: rgb(66,133,244);\" target=\"_blank\" href="
"\"${base_url | safe }/web#id=${process.id}&model=civil.service.tracker.selection.process\" title="
"\"${ process.name or '' }\" aria-label=\"${ process.name or '' }\">\n"
"              ${ process.name or '' }\n"
"            </a>\n"
"          </h3>\n"
"        </th>\n"
"      </tr>\n"
"      <tr>\n"
"        <th style=\"line-height: 125%; vertical-align: top; background-color: #EFEFEF; text-"
"align: left; padding: 6px 12px; border: 1px solid #CCC;\">Event</th>\n"
"        <th style=\"line-height: 125%; vertical-align: top; background-color: #EFEFEF; text-"
"align: center; padding: 6px 12px; border: 1px solid #CCC;\">Date</th>\n"
"        <th style=\"line-height: 125%; vertical-align: top; background-color: #EFEFEF; text-"
"align: left; padding: 6px 12px; border: 1px solid #CCC;\">Type</th>\n"
"        <th style=\"line-height: 125%; vertical-align: top; background-color: #EFEFEF; text-"
"align: left; padding: 6px 12px; border: 1px solid #CCC;\">Description</th>\n"
"        <th style=\"line-height: 125%; vertical-align: top; background-color: #EFEFEF; text-"
"align: center; padding: 6px 12px; border: 1px solid #CCC;\">Doc.</th>\n"
"      </tr>\n"
"    </thead>\n"
"    <tbody>\n"
