"      <tr>\n"
"        <td style=\"line-height: 125%; vertical-align: top; text-align: left; padding: 6px 12px; "
"border: 1px solid #CCC; white-space: nowrap;\">\n"
"          <a style=\"text-decoration: none; color: inherit;\" target=\"_blank\" href="
"\"${base_url | safe}/web#id=${event.id}&model=civil.service.tracker.process.event\" title="
"\"${ event.name or '' }\" aria-label=\"${ event.name or '' }\">\n"
"            ${ event.name or '' }\n"
"          </a>\n"
